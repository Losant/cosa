{
  "name": "cosa",
  "version": "2.8.2",
  "description": "Cosa Models for MongoDB",
  "main": "lib/index.js",
  "engines": {
    "node": ">=8.9.0",
    "yarn": ">=1.2.1"
  },
  "scripts": {
    "preinstall": "node -e 'if(!process.env.npm_execpath.includes(`yarn`)){console.error(`Use yarn!`);process.exit(1);}'",
    "reinstall": "rm -rf node_modules && yarn install",
    "test": "mocha --require test/support/env",
    "test-debug": "mocha --debug-brk --require test/support/env",
    "test-cov": "istanbul cover _mocha -- --require test/support/env",
    "doc": "documentation build lib/index.js -f md --github > API.md"
  },
  "author": {
    "name": "Adam Daniel",
    "email": "adam@acdaniel.com"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/acdaniel/cosa"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "bson": "~1.0.4",
    "clone": "^2.1.1",
    "debug": "^3.0.1",
    "defaults": "^1.0.3",
    "error": "^7.0.2",
    "es6-promisify": "^6.0.0",
    "etag": "^1.8.1",
    "joi": "^10.6.0",
    "mongodb": "~2.2.31",
    "mongodb-extended-json": "~1.10.0",
    "object-assign": "^4.1.1",
    "object-path": "^0.11.4",
    "q": "^1.5.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-datetime": "^1.5.0",
    "documentation": "^5.3.5",
    "eslint": "^4.6.1",
    "eslint-plugin-mocha": "^4.8.0",
    "eslint-plugin-node": "^5.1.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.3",
    "string-template": "^1.0.0"
  }
}
